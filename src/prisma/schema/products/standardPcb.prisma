model StandardPcb {
   id        String   @id
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   name String

   buildTime    Int
   weight       Float
   initialPrice Float?
   finalPrice   Float?

   boardType         SPBoardType
   xoutAllowance     Boolean?
   routeProcess      SPRouteProcess?
   breakAwayRail     Boolean?
   panelRequirements String?

   differentDesign Int
   length          Float
   width           Float
   quantity        Int

   layers      Int
   layerOrder  String[]
   copperLayer SPCopperLayer?
   solderMask  SPSolderMask?
   silkscreen  SPSilkscreen?

   material                 SPMaterial
   fr4TG                    SPFr4TG?
   thermalConductivity      Float?
   thermoElectricSeparation Boolean?
   rogers                   SPRogers?
   structureOfMCPCB         spStructureMCPCB?

   thickness   Float
   minTrack    Float
   minHoleSize Float

   solderMaskColor SPSolderMaskColor
   silkscreenColor SPSilkscreenColor
   uvPrinting      SPUVPrinting
   edgeConnector   Boolean

   surfaceFinish      String
   surfaceFinishLayer SPSurfaceFinishLayer
   surfaceThickness   Float?
   acceptHASLChange   Boolean?

   finishedCopper  Float
   innerCopper     Float?
   removeProductNo SPRemoveProductNo

   peelableSolderMask  SPPeelableSolderMask
   holeCopperThickness Float
   ulMarking           SPUlMarking?
   dateCode            SPDateCode
   dateCodeDescription String?

   customizedServices SPCustomizedServices[]
   specialRequests    String

   orderId String
   order   Order  @relation(fields: [orderId], references: [id], onDelete: Cascade)

   fabricationStatuses ProductFabricationStatus[]
   files               ProductFiles[]
}

enum SPBoardType {
   SINGLE_PIECES
   PANEL_BY_CUSTOMER
   PANEL_BY_VELENOVA
}

enum SPRouteProcess {
   VELENOVA_PREFER
   V_SCORING
   TAB_ROUTE
   V_SCORING_AND_TAB_ROUTE
}

enum SPCopperLayer {
   TOP_LAYER
   BOTTOM_LAYER
   NONE
}

enum SPSolderMask {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum SPSilkscreen {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum SPMaterial {
   FR_4
   ALUMINUM
   ROGERS
   HDI
   COPPER_BASE
}

enum SPFr4TG {
   TG130_140
   TG150_160
   TG170_180
   S1000H_TG150
   S1000_2M_TG170
}

enum SPRogers {
   ROGERS_5880
   ROGERS_4350
   ROGERS_4003
}

enum spStructureMCPCB {
   METAL_CORE_MIDDLE
   METAL_BASE_BOTTOM
}

enum SPSolderMaskColor {
   GREEN
   RED
   BLUE
   BLACK
   NONE
}

enum SPSilkscreenColor {
   WHITE
   BLACK
   YELLOW
   NONE
}

enum SPUVPrinting {
   SINGLE_SIDED_TOP
   SINGLE_SIDED_BOTTOM
   DOUBLE_SIDED
   NONE
}

enum SPSurfaceFinish {
   HASL_WITH_LEAD
   HASL_LEAD_FREE
   IMMERSION_GOLD
   OSP
   HARD_GOLD
   IMMERSION_SILVER
   IMMERSION_TIN
   HASL_LEAD_FREE_SELECTIVE_IMMERSION_GOLD
   HASL_LEAD_FREE_SELECTIVE_HARD_GOLD
   IMMERSION_GOLD_SELECTIVE_HARD_GOLD
   ENEPIG
   PLAIN_COPPER
}

enum SPSurfaceFinishLayer {
   SINGLE_SIDE
   DOUBLE_SIDE
}

enum SPRemoveProductNo {
   YES
   NO
   SPECIFY_LOCATION
}

enum SPPeelableSolderMask {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum SPUlMarking {
   TOP_SILK_SCREEN
   BOTTOM_SILK_SCREEN
   TOP_SOLDER_MASK
   BOTTOM_SOLDER_MASK
   TOP_COPPER
   BOTTOM_COPPER
   DEFAULT
   NONE
}

enum SPDateCode {
   VELENOVA_PREFER
   CUSTOMER_REQUIRED
   UPDATE_DESIGNED_DATE_CODE
   NONE
}

enum SPCustomizedServices {
   HALF_CUT
   EDGE_PLATING
   IMPEDANCE_CONTROL
   HALOGEN_FREE
   CUSTOM_STACKUP
   CARBON_INK
   ALL_VIAS_FILLED_WITH_RESIN_AND_CAPPED
   VIA_IN_PAD
   PRESS_FIT_HOLES
   COUNTERSINK
   Z_AXIS_MILLING
   BLACK_FR4
   EMBEDDED_COPPER_PCB
   CAVITY_PCB
   SEMI_FLEX_PCB
   WHOLE_PARTIALLY_HYBRID_PCB
   BACK_DRILL_PCB
   LEADLESS_PARTIALLY_PLATED_HARD_GOLD
}
