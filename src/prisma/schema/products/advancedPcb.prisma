model AdvancedPcb {
   id        String   @id @default(nanoid())
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   name     String
   fileName String?

   buildTime    Int
   weight       Float
   initialPrice Float
   finalPrice   Float?

   pcbType           APType
   boardSpec         APBoardSpec
   boardType         APBoardType
   xoutAllowance     Boolean?
   routeProcess      APRouteProcess?
   breakAwayRail     Boolean?
   panelRequirements String?

   differentDesign Int
   length          Float
   width           Float
   quantity        Int

   layers      Int
   layerOrder  String[]
   copperLayer APCopperLayer?
   solderMask  APSolderMask?
   silkscreen  APSilkscreen?

   material      APMaterial
   highSpeed     String?
   highFrequency String?

   thickness   Float
   minTrack    Float
   minHoleSize Float

   solderMaskColor APSolderMaskColor
   silkscreenColor APSilkscreenColor
   edgeConnector   Boolean

   surfaceFinish    APSurfaceFinish
   surfaceThickness Float?
   finishedCopper   Float
   innerCopper      Float?

   peelableSolderMask  APPeelableSolderMask
   holeCopperThickness Float
   ulMarking           APUlMarking?
   dateCode            APDateCode
   dateCodeDescription String?

   customizedServices    APCustomizedServices[]
   finalInspectionReport APFinalInspectionReport[]
   specialRequests       String

   orderId String
   order   Order  @relation(fields: [orderId], references: [id], onDelete: Cascade)

   productionStatuses ProductProductionStatus[]
}

enum APType {
   THROUGH_HOLE
   HDI
}

enum APBoardSpec {
   IPC_6012_CLASS_2
   IPC_6012_CLASS_3
   IATF_16949
   ISO_13485
   CUSTOMER_STANDARD
}

enum APBoardType {
   SINGLE_PIECES
   PANEL_BY_CUSTOMER
   PANEL_BY_VELENOVA
}

enum APRouteProcess {
   VELENOVA_PREFER
   V_SCORING
   TAB_ROUTE
   V_SCORING_AND_TAB_ROUTE
}

enum APCopperLayer {
   TOP_LAYER
   BOTTOM_LAYER
   NONE
}

enum APSolderMask {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum APSilkscreen {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum APMaterial {
   TG140_FR4
   TG150_FR4
   TG170_FR4
   TG150_FR4_HALOGEN_FREE
   TG170_FR4_HALOGEN_FREE
   HIGH_CTI
   HIGH_CTI_HALOGEN_FREE
   HIGH_SPEED
   HIGH_FREQUENCY
}

enum APSolderMaskColor {
   GREEN
   RED
   YELLOW
   BLUE
   WHITE
   BLACK
   PINK
   GREY
   ORANGE
   PURPLE
   MATTE_BLACK
   MATTE_GREEN
   MATTE_BLUE
   MATTE_RED
   TRANSPARENT
   NONE
}

enum APSilkscreenColor {
   WHITE
   BLACK
   YELLOW
   BLUE
   GREY
   NONE
}

enum APSurfaceFinish {
   HASL_LEAD_FREE
   IMMERSION_GOLD
   OSP
   HARD_GOLD
   IMMERSION_SILVER
   IMMERSION_TIN
   HASL_LEAD_FREE_SELECTIVE_IMMERSION_GOLD
   HASL_LEAD_FREE_SELECTIVE_HARD_GOLD
   IMMERSION_GOLD_SELECTIVE_HARD_GOLD
   ENEPIG
   PLAIN_COPPER
}

enum APPeelableSolderMask {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum APUlMarking {
   TOP_SILK_SCREEN
   BOTTOM_SILK_SCREEN
   TOP_SOLDER_MASK
   BOTTOM_SOLDER_MASK
   TOP_COPPER
   BOTTOM_COPPER
   DEFAULT
   NONE
}

enum APDateCode {
   VELENOVA_PREFER
   CUSTOMER_REQUIRED
   UPDATE_DESIGNED_DATE_CODE
   NONE
}

enum APCustomizedServices {
   HALF_CUT
   EDGE_PLATING
   IMPEDANCE_CONTROL
   CUSTOM_STACKUP
   CARBON_INK
   ALL_VIAS_FILLED_WITH_RESIN_AND_CAPPED
   VIA_IN_PAD
   COUNTERSINK
   Z_AXIS_MILLING
   PRESS_FIT_HOLES
   VIA_FILLED_WITH_COPPER
   BLACK_FR4
   EMBEDDED_COPPER_PCB
   CAVITY_PCB
   SEMI_FLEX_PCB
   WHOLE_PARTIALLY_HYBRID_PCB
   BACK_DRILL_PCB
   LEADLESS_PARTIALLY_PLATED_HARD_GOLD
}

enum APFinalInspectionReport {
   DEFAULT_INSPECTION_REPORT
   MICROSECTION_INSPECTION_REPORT
   SOLDERABILITY_INSPECTION_REPORT
   THERMAL_STRESS_INSPECTION_REPORT
   IMPEDANCE_INSPECTION_REPORT
   HUMIDITY_INDICATOR_CARD
}
