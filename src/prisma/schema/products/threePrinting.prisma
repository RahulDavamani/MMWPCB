model ThreePrinting {
   id        String   @id @default(nanoid())
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   name     String
   fileName String?

   weight       Float
   initialPrice Float?
   finalPrice   Float?

   quantity           Int[]
   designUnits        TPDesignUnit
   material           TPMaterial
   materialType       TPMaterialType
   color              TPColor
   process            TPProcess
   surfaceFinish      TPSurfaceFinish
   surfaceFinishColor String?

   technicalDrawing   String?
   threadsTappedHoles Boolean
   inserts            Boolean

   silkScreen         String
   laserEngraving     Boolean
   partsAssembly      TPPartsAssembly
   wallThicknessRisk  Boolean
   printedThreadsRisk Boolean

   productDescription TPProductDescription
   specialRequests    String

   orderId String
   order   Order  @relation(fields: [orderId], references: [id], onDelete: Cascade)

   fabricationStatuses ProductFabricationStatus[]
}

enum TPDesignUnit {
   MM
   CM
   INCH
}

enum TPMaterial {
   ALUMINUM
   STAINLESS_STEEL
   NYLON
   ABS
   PETG
}

enum TPMaterialType {
   ALUMINUM
   STAINLESS_STEEL
   PA_12
   HP_PA_12
   GLASS_FIBER_NYLON
   ABS
   STRATASYS_ABS
   PETG
}

enum TPColor {
   METALLIC
   SILVER
   WHITE
   DARK_BLACK
   BLACK
   SILVER_GRAY
   RED
   BLUE
   YELLOW
   GREEN
}

enum TPProcess {
   SLM
   SLS
   FDM
}

enum TPSurfaceFinish {
   NONE
   SPRAY_PAINTING_MATT
   SPRAY_PAINTING_HIGH_GLOSS
   SANDING_1000
   DYING
}

enum TPPartsAssembly {
   NO
   ASSEMBLY_TEST
   SHIP_ASSEMBLY
}

enum TPProductDescription {
   PRINTER_ENCLOSURE
   INK_CARTRIDGE
   MOTORCYCLE_CONNECTOR_ENCLOSURE
   CNC_LATHE_REPLACEMENT_ARM
   HEADBAND
   WHEEL_PLATE
   RECORDER_ENCLOSURE
   COMPOUND_OPTICAL_MICROSCOPE
}
