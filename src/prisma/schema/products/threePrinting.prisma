model ThreePrinting {
   id        String   @id
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   name String

   weight       Float
   initialPrice Float?
   finalPrice   Float?

   quantity    Int[]
   designUnits TPDesignUnit
   material    TPMaterial
   color       TPColor
   process     TPProcess

   technicalDrawing   String?
   threadsTappedHoles Boolean
   inserts            Boolean

   silkScreen         String
   laserEngraving     Boolean
   partsAssembly      TPPartsAssembly
   wallThicknessRisk  Boolean
   printedThreadsRisk Boolean

   productDescription TPProductDescription
   specialRequests    String

   orderId String
   order   Order  @relation(fields: [orderId], references: [id], onDelete: Cascade)

   fabricationStatuses ProductFabricationStatus[]
   files               ProductFiles[]
}

enum TPDesignUnit {
   MM
   CM
   UM
   INCH
}

enum TPMaterial {
   ALUMINUM
   STAINLESS_STEEL
   NYLON
   ABS
   PETG
   CO_CR_ALLOY
   PURE_TITANIUM
   TITANIUM_ALLOY
   ALUMINUM_ALLOY
   HIGH_TEMPERATURE_NICKEL_ALLOY
   OTHERS
}

enum TPColor {
   METALLIC
   SILVER
   WHITE
   DARK_BLACK
   BLACK
   SILVER_GRAY
   RED
   BLUE
   YELLOW
   GREEN
}

enum TPProcess {
   SLM
   SLS
   FDM
}

enum TPPartsAssembly {
   NO
   ASSEMBLY_TEST
   SHIP_ASSEMBLY
}

enum TPProductDescription {
   PRINTER_ENCLOSURE
   INK_CARTRIDGE
   MOTORCYCLE_CONNECTOR_ENCLOSURE
   CNC_LATHE_REPLACEMENT_ARM
   HEADBAND
   WHEEL_PLATE
   RECORDER_ENCLOSURE
   COMPOUND_OPTICAL_MICROSCOPE
}
