   rigidFlexId String?
   rigidFlex   RigidFlex? @relation(fields: [rigidFlexId], references: [id], onDelete: Cascade)

   assemblyId String?
   assembly   Assembly? @relation(fields: [assemblyId], references: [id], onDelete: Cascade)

   stencilId String?
   stencil   Stencil? @relation(fields: [stencilId], references: [id], onDelete: Cascade)

   cncId String?
   cnc   CNC?    @relation(fields: [cncId], references: [id], onDelete: Cascade)

   sheetMetalId String?
   sheetMetal   SheetMetal? @relation(fields: [sheetMetalId], references: [id], onDelete: Cascade)

   threePrintingId String?
   threePrinting   ThreePrinting? @relation(fields: [threePrintingId], references: [id], onDelete: Cascade)

   injectionMoldingId String?
   injectionMolding   InjectionMolding? @relation(fields: [injectionMoldingId], references: [id], onDelete: Cascade)

   vacuumCastingId String?
