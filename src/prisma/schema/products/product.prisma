model ProductFabricationStatus {
   id        String   @id @default(nanoid())
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   num         Int
   status      String
   completed   Boolean
   completedAt DateTime?

   standardPcbId String?
   standardPcb   StandardPcb? @relation(fields: [standardPcbId], references: [id])

   advancedPcbId String?
   advancedPcb   AdvancedPcb? @relation(fields: [advancedPcbId], references: [id])

   flexiblePcbId String?
   flexiblePcb   FlexiblePcb? @relation(fields: [flexiblePcbId], references: [id])

   rigidFlexId String?
   rigidFlex   RigidFlex? @relation(fields: [rigidFlexId], references: [id])

   assemblyId String?
   assembly   Assembly? @relation(fields: [assemblyId], references: [id])

   stencilId String?
   stencil   Stencil? @relation(fields: [stencilId], references: [id])
}
