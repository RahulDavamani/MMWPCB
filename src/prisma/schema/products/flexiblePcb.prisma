model FlexiblePcb {
   id        String   @id
   createdAt DateTime @default(now())
   updatedAt DateTime @updatedAt

   name String

   buildTime    Int
   weight       Float
   initialPrice Float?
   finalPrice   Float?

   boardType         FPBoardType
   xoutAllowance     Boolean?
   routeProcess      FPRouteProcess?
   breakAwayRail     Boolean?
   panelRequirements String?

   differentDesign Int
   length          Float
   width           Float
   quantity        Int
   layers          Int

   material FPMaterial
   petType  FPPetType?

   thickness   Float
   minTrack    Float
   minHoleSize Float

   solderMaskColor FPSolderMaskColor
   silkscreenColor FPSilkscreenColor
   edgeConnector   Boolean
   stiffener       FPStiffener

   surfaceFinish    String
   surfaceThickness Float[]
   finishedCopper   Float

   etest            Boolean
   tape             FPTape
   doubleSidedTape  FPDoubleSidedTape
   emiShieldingFilm FPEmiShieldingFilm

   peelableSolderMask  FPPeelableSolderMask
   ulMarking           FPUlMarking
   dateCode            FPDateCode
   dateCodeDescription String?

   customizedServices FPCustomizedServices[]
   specialRequests    String

   orderId String
   order   Order  @relation(fields: [orderId], references: [id], onDelete: Cascade)

   fabricationStatuses ProductFabricationStatus[]
   files               ProductFiles[]
}

enum FPBoardType {
   SINGLE_PIECES
   PANEL_BY_CUSTOMER
   PANEL_BY_VELENOVA
}

enum FPRouteProcess {
   VELENOVA_PREFER
   V_SCORING
   TAB_ROUTE
   V_SCORING_AND_TAB_ROUTE
}

enum FPMaterial {
   POLYIMIDE_FLEX
   PET
   HIGH_FREQUENCY
}

enum FPPetType {
   TRANSPARENT
   TRANSLUCENT
}

enum FPSolderMaskColor {
   YELLOW_COVERLAY
   WHITE_COVERLAY
   BLACK_COVERLAY
   TRANSPARENT
   NONE
}

enum FPSilkscreenColor {
   WHITE
   BLACK
   NONE
}

enum FPStiffener {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum FPSurfaceFinish {
   IMMERSION_GOLD
   OSP
   HARD_GOLD
   IMMERSION_SILVER
   IMMERSION_TIN
   IMMERSION_GOLD_SELECTIVE_HARD_GOLD
   ENEPIG
}

enum FPTape {
   THREEM_467
   TESA_8853
   TESA_8854
   THREEM_9495LE
   NONE
}

enum FPDoubleSidedTape {
   HT_A1134
   NONE
}

enum FPEmiShieldingFilm {
   HCF_6000G
   PC800
   NONE
}

enum FPPeelableSolderMask {
   TOP_SIDE
   BOTTOM_SIDE
   BOTH_SIDES
   NONE
}

enum FPUlMarking {
   TOP_SILK_SCREEN
   BOTTOM_SILK_SCREEN
   TOP_SOLDER_MASK
   BOTTOM_SOLDER_MASK
   TOP_COPPER
   BOTTOM_COPPER
   DEFAULT
   NONE
}

enum FPDateCode {
   VELENOVA_PREFER
   CUSTOMER_REQUIRED
   UPDATE_DESIGNED_DATE_CODE
   NONE
}

enum FPCustomizedServices {
   HALF_CUT
   EDGE_PLATING
   IMPEDANCE_CONTROL
   VIA_FILLED_WITH_COPPER
   FPC_WITH_AIRGAP
   SINGLE_SIDE_DOUBLE_ACCESS
   STIFFENER_BETWEEN_EDGE_CONNECTOR
}
